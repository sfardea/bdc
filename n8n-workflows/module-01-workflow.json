      "name": "Claude - Generate Insights",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [1050, 200],
      "credentials": {
        "httpHeaderAuth": {
          "id": "2",
          "name": "Anthropic API"
        }
      }
    },
    {
      "parameters": {
        "functionCode": "// Parse Claude response and format insights\nconst claudeResponse = items[0].json;\nconst userData = items[0].json.originalData;\n\nconst insights = {\n  userId: userData.sessionId,\n  module: 'BC_Module_01',\n  timestamp: new Date().toISOString(),\n  profile: userData.data,\n  aiInsights: claudeResponse.content[0].text,\n  nextSteps: [\n    {\n      module: 'BC_Module_02',\n      priority: 'high',\n      reason: 'Module suivant dans le parcours'\n    }\n  ],\n  personalizedTips: []\n};\n\n// Add personalized tips based on profile\nif (userData.data.situationPro === 'recherche-emploi') {\n  insights.personalizedTips.push({\n    type: 'focus',\n    message: 'Concentrez-vous particuli√®rement sur les modules d\\'exploration professionnelle'\n  });\n}\n\nif (userData.data.age < 25) {\n  insights.personalizedTips.push({\n    type: 'advice',\n    message: 'Les modules sur les motivations et valeurs seront particuli√®rement importants pour vous'\n  });\n}\n\nreturn [insights];"
      },
      "name": "Format AI Insights",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [1250, 200]
    },
